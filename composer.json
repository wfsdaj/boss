{
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/",
            "boss\\": "boss/"
        },
        "files": [
            "boss/functions.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}