{
    "require": {
        "php": ">=7.4",
        "topthink/think-template": "2.0.10"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/",
            "boss\\": "boss/"
        },
        "files": [
            "boss/functions.php",
            "app/helpers.php",
            "app/events.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
